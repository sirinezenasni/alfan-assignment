{
  "name": "alfan-assignment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run start-development",
    "start-prod": "export NODE_ENV=production && node ./bin/www",
    "start-development": "export NODE_ENV=development && npm run start-node",
    "start-test": "export NODE_ENV=test && npm run start-node",
    "start-node": "nodemon ./bin/www",
    "migrate-db-dev": "export NODE_ENV=development && npx sequelize-cli db:migrate",
    "migrate-db-test": "export NODE_ENV=test && npx sequelize-cli db:migrate",
    "migrate-db-prod": "export NODE_ENV=production && npx sequelize-cli db:migrate",
    "empty-database-test": "export NODE_ENV=test && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "test": "export NODE_ENV=test && mocha --exit \"./{,!(node_modules)/**/}*.test.js\"",
    "clean-and-test": "npm run empty-database-test && npm run test"
  },
  "dependencies": {
    "ajv": "^8.6.0",
    "ajv-formats": "^2.1.0",
    "aws-sdk": "^2.951.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "~1.6.3",
    "jade": "^0.31.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "rand-token": "^1.0.1",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  }
}
